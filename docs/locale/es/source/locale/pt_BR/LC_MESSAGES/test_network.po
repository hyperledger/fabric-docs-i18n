#~ msgstr ""

#~ msgid "############ Create Org1 Identities ######################"
#~ msgstr ""

#~ msgid ""
#~ "cryptogen generate --config=./organizations/cryptogen"
#~ "/crypto-config-org1.yaml --output=organizations"
#~ msgstr ""

#~ msgid ""
#~ "org1.example.com + res=0 + set +x "
#~ "########################################################## "
#~ "############ Create Org2 Identities "
